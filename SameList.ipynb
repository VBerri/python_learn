    "def samelist(lst):\n",
    "    status = False \n",
    "    for i in range(len(lst) - 1):\n",
    "        if lst[i] == lst[i+1]:\n",
    "            status = True\n",
    "        else:\n",
    "           status = False\n",
    "           break\n",
    "    print(status)\n",
    "samelist([1, 2, 2, 2])        "
   ]
