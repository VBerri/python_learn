    "def love():\n",
    "    num = int(input(\"Enter how many times the sequence should repeat\"))\n",
    "    for i in range(1, num + 1):\n",
    "        if i % 2 == 0:\n",
    "            print(\"Loves me not\")\n",
    "        else:\n",
    "            print(\"Loves me\")\n",
    "love()"
