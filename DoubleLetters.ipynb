   "source": [
    "def doubleletters(string):\n",
    "  d = []  \n",
    "  for char in string:\n",
    "      d.append(char) \n",
    "  if len(d) != len(set(d)):\n",
    "      print(\"This word has two or more duplicate letters\")\n",
    "  else:\n",
    "      print(\"This list does not have duplicate letters\")\n",
    "doubleletters(\"Cattle\")\n",
    "        "
