    "def syllablechecker(strin):\n",
    "    lst = list(strin)\n",
    "    syllables = lst.count(\"-\")\n",
    "    syllables += 1\n",
    "    print(\"The number of syllables are: \", syllables)\n",
    "syllablechecker(\"Ill-ite-rat-e\")"
